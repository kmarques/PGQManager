{
    "sEcho": {{ sEcho }},
    "iTotalRecords": "{{ iTotalRecords }}",
    "iTotalDisplayRecords": "{{ records|length }}",
    "aaData": [
        {% for row in records %}
            [
                "{{ row.database }}",
                "{{ row.queue }}",
                "{{ row.consumer }}",
                {{ row.eventId }},
                "{{ row.failedDate }}",
                {{ row.eventRetry }},
                "{{ row.createdDate }}"
            ]{% if not loop.last %}},{% endif %}
        {% endfor %}
      ]
    "
}